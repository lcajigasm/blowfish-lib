{
    "name": "luisinder/blowfish-lib",
    "version": "0.2.0",
    "description": "Simple helper library for hashing and verifying passwords using Blowfish (bcrypt).",
    "type": "library",
    "keywords": ["blowfish", "bcrypt", "password", "hash"],
    "homepage": "https://github.com/luisinder/blowfish-lib",
    "license": "MIT",
    "authors": [
        {
            "name": "Luis Cajigas",
            "role": "Developer"
        }
    ],
    "support": {
        "issues": "https://github.com/luisinder/blowfish-lib/issues"
    },
    "require": {
        "php": ">=7.4"
    },
    "autoload": {
        "files": ["src/blowFish.php"],
        "psr-4": {
            "Luisinder\\BlowfishLib\\": "src/"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^9.6"
    },
    "autoload-dev": {
        "psr-4": {
            "Luisinder\\BlowfishLib\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "example": "php examples/example.php",
        "test": "phpunit"
    }
}